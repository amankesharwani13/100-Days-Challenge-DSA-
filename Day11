53. Maximum Subarray
class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int MaxSum = nums[0];
        int currSum = 0;
        for(int i=0;i<nums.size();i++){
            currSum += nums[i];
            MaxSum = max(MaxSum, currSum);
            if(currSum < 0)currSum = 0;
        }
        return MaxSum;
    }
};
